{
  "name": "fdk-kit",
  "description": "A collection of reusable UI components for Felles datakatalog.",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:fellesdatakatalog/fdk-kit.git"
  },
  "author": {
    "name": "Alexander Zakharov (azaxarov)",
    "email": "alexander.zakharov@capgemini.com"
  },
  "contributors": [
    {
      "name": "Jeff Reiffers",
      "email": "jeff@ouvir.no"
    }
  ],
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 14.15.0"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "lint:project": "eslint --ext tsx,ts,jsx,js,json,md,html ./",
    "lint:project:fix": "eslint --ext tsx,ts,jsx,js,json,md,html ./ --fix",
    "lint:staged:fix": "eslint --ext tsx,ts,jsx,js,json,md,html --fix",
    "type:check": "tsc",
    "test": "run-s clean:coverage test:jest",
    "test:verbose": "run-s clean:coverage test:jest:verbose",
    "test:coverage": "run-s clean:coverage test:jest:coverage",
    "test:jest": "jest --passWithNoTests",
    "test:jest:verbose": "jest --passWithNoTests --verbose",
    "test:jest:coverage": "jest --passWithNoTests --coverage",
    "test:watch": "jest --passWithNoTests --watch",
    "test:watch:all": "jest --passWithNoTests --watchAll",
    "test:watch:verbose": "jest --passWithNoTests --watch --verbose",
    "test:watch:all:verbose": "jest --passWithNoTests --watchAll --verbose",
    "clean:coverage": "rm -rf coverage",
    "commit": "git-cz",
    "storybook": "start-storybook -p 6006 --ci",
    "storybook:build": "build-storybook",
    "storybook:deploy": "storybook-to-ghpages --script storybook:build --ci --out storybook-static"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "storybook-deployer": {
    "gitUsername": "FDK Bot",
    "gitEmail": "fdk-bot@digdir.no",
    "commitMessage": "chore(project): deploy Storybook to GitHub Pages [skip ci]"
  },
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "hosted-git-info": "^4.1.0",
    "glob-parent": "^5.1.2",
    "nth-check": "^2.0.1",
    "postcss": "^8.4.5",
    "trim": "^1.0.1",
    "tar": "^6.1.11"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@skatteetaten/frontend-components": "^5.0.4",
    "react": "^17.0.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@storybook/addon-a11y": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addons": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/storybook-deployer": "^2.8.10",
    "@storybook/theming": "^6.4.19",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.18.9",
    "@types/react-scroll": "^1.8.3",
    "@types/styled-components": "^5.1.21",
    "@types/webpack": "^5.28.0",
    "@types/webpack-merge": "^5.0.0",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^1.13.3",
    "chromatic": "^6.4.1",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.6",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.8",
    "jest-svg-transformer": "^1.0.0",
    "lerna": "^4.0.0",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "react-dom": "^17.0.2",
    "react-scroll": "^1.8.4",
    "react-svg-loader": "^3.0.3",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  }
}

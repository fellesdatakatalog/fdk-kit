{
  "name": "fdk-kit",
  "description": "A collection of reusable UI components for Felles datakatalog.",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:fellesdatakatalog/fdk-kit.git"
  },
  "author": {
    "name": "Alexander Zakharov (azaxarov)",
    "email": "alexander.zakharov@capgemini.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 14.15.0"
  },
  "scripts": {
    "lint:project": "eslint --ext tsx,ts,jsx,js,json,md,html ./",
    "lint:project:fix": "eslint --ext tsx,ts,jsx,js,json,md,html ./ --fix",
    "lint:staged:fix": "eslint --ext tsx,ts,jsx,js,json,md,html --fix",
    "type:check": "tsc",
    "test": "run-s clean:coverage test:jest",
    "test:verbose": "run-s clean:coverage test:jest:verbose",
    "test:coverage": "run-s clean:coverage test:jest:coverage",
    "test:jest": "jest --passWithNoTests",
    "test:jest:verbose": "jest --passWithNoTests --verbose",
    "test:jest:coverage": "jest --passWithNoTests --coverage",
    "test:watch": "jest --passWithNoTests --watch",
    "test:watch:all": "jest --passWithNoTests --watchAll",
    "test:watch:verbose": "jest --passWithNoTests --watch --verbose",
    "test:watch:all:verbose": "jest --passWithNoTests --watchAll --verbose",
    "clean:coverage": "rm -rf coverage",
    "commit": "git-cz",
    "storybook": "start-storybook -p 6006 --ci",
    "storybook:build": "build-storybook"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "react": "^17.0.1",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@storybook/addon-a11y": "^6.1.0-alpha.31",
    "@storybook/addon-essentials": "^6.1.0-alpha.31",
    "@storybook/addon-links": "^6.1.0-alpha.31",
    "@storybook/addons": "^6.1.0-alpha.31",
    "@storybook/react": "^6.1.0-alpha.31",
    "@storybook/theming": "^6.1.0-alpha.31",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.5",
    "@types/styled-components": "^5.1.4",
    "@types/webpack": "^4.41.23",
    "@types/webpack-merge": "^4.1.5",
    "@types/webpack-node-externals": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.2",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "jest-styled-components": "^7.0.3",
    "jest-svg-transformer": "^1.0.0",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "react-dom": "^17.0.1",
    "react-svg-loader": "^3.0.3",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.7",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.2.0",
    "webpack-node-externals": "^2.5.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  }
}

{
  "name": "fdk-kit",
  "description": "A collection of reusable UI components for Felles datakatalog.",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:fellesdatakatalog/fdk-kit.git"
  },
  "author": {
    "name": "Alexander Zakharov (azaxarov)",
    "email": "alexander.zakharov@capgemini.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 14.15.0"
  },
  "scripts": {
    "lint:project": "eslint --ext tsx,ts,jsx,js,json,md,html ./",
    "lint:project:fix": "eslint --ext tsx,ts,jsx,js,json,md,html ./ --fix",
    "lint:staged:fix": "eslint --ext tsx,ts,jsx,js,json,md,html --fix",
    "type:check": "tsc",
    "test": "run-s clean:coverage test:jest",
    "test:verbose": "run-s clean:coverage test:jest:verbose",
    "test:coverage": "run-s clean:coverage test:jest:coverage",
    "test:jest": "jest --passWithNoTests",
    "test:jest:verbose": "jest --passWithNoTests --verbose",
    "test:jest:coverage": "jest --passWithNoTests --coverage",
    "test:watch": "jest --passWithNoTests --watch",
    "test:watch:all": "jest --passWithNoTests --watchAll",
    "test:watch:verbose": "jest --passWithNoTests --watch --verbose",
    "test:watch:all:verbose": "jest --passWithNoTests --watchAll --verbose",
    "clean:coverage": "rm -rf coverage",
    "commit": "git-cz",
    "storybook": "start-storybook -p 6006 --ci",
    "storybook:build": "build-storybook",
    "storybook:deploy": "storybook-to-ghpages --script storybook:build --ci --out storybook-static"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "storybook-deployer": {
    "gitUsername": "FDK Bot",
    "gitEmail": "fdk-bot@digdir.no",
    "commitMessage": "chore(project): deploy Storybook to GitHub Pages [skip ci]"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "react": "^17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@storybook/addon-a11y": "^6.1.16",
    "@storybook/addon-essentials": "^6.1.16",
    "@storybook/addon-links": "^6.1.16",
    "@storybook/addons": "^6.1.16",
    "@storybook/react": "^6.1.16",
    "@storybook/storybook-deployer": "^2.8.7",
    "@storybook/theming": "^6.1.16",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/react-scroll": "^1.8.2",
    "@types/styled-components": "^5.1.7",
    "@types/webpack": "^4.41.26",
    "@types/webpack-merge": "^4.1.5",
    "@types/webpack-node-externals": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-loader": "^8.2.1",
    "babel-plugin-styled-components": "^1.12.0",
    "chromatic": "^5.6.1",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "jest-svg-transformer": "^1.0.0",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "react-dom": "^17.0.1",
    "react-scroll": "^1.8.1",
    "react-svg-loader": "^3.0.3",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  }
}

{
  "name": "fdk-kit",
  "description": "A collection of reusable UI components for Felles datakatalog.",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:fellesdatakatalog/fdk-kit.git"
  },
  "author": {
    "name": "Alexander Zakharov (azaxarov)",
    "email": "alexander.zakharov@capgemini.com"
  },
  "contributors": [
    {
      "name": "Jeff Reiffers",
      "email": "jeff@ouvir.no"
    }
  ],
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 14.15.0"
  },
  "scripts": {
    "preinstall": "([ ! -f package-lock.json ] && npm install --package-lock-only --ignore-scripts --no-audit); npx npm-force-resolutions",
    "lint:project": "eslint --ext tsx,ts,jsx,js,json,md,html ./",
    "lint:project:fix": "eslint --ext tsx,ts,jsx,js,json,md,html ./ --fix",
    "lint:staged:fix": "eslint --ext tsx,ts,jsx,js,json,md,html --fix",
    "type:check": "tsc",
    "test": "run-s clean:coverage test:jest",
    "test:verbose": "run-s clean:coverage test:jest:verbose",
    "test:coverage": "run-s clean:coverage test:jest:coverage",
    "test:jest": "jest --passWithNoTests",
    "test:jest:verbose": "jest --passWithNoTests --verbose",
    "test:jest:coverage": "jest --passWithNoTests --coverage",
    "test:watch": "jest --passWithNoTests --watch",
    "test:watch:all": "jest --passWithNoTests --watchAll",
    "test:watch:verbose": "jest --passWithNoTests --watch --verbose",
    "test:watch:all:verbose": "jest --passWithNoTests --watchAll --verbose",
    "clean:coverage": "rm -rf coverage",
    "commit": "git-cz",
    "storybook": "start-storybook -p 6006 --ci",
    "storybook:build": "build-storybook",
    "storybook:deploy": "storybook-to-ghpages --script storybook:build --ci --out storybook-static"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "storybook-deployer": {
    "gitUsername": "FDK Bot",
    "gitEmail": "fdk-bot@digdir.no",
    "commitMessage": "chore(project): deploy Storybook to GitHub Pages [skip ci]"
  },
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "hosted-git-info": "^4.1.0",
    "glob-parent": "^5.1.2",
    "nth-check": "^2.0.1",
    "postcss": "^8.4.5",
    "trim": "^1.0.1",
    "trim-newlines": "^3.0.1",
    "tar": "^6.1.11",
    "parse-path": "^5.0.0",
    "parse-url": "^8.1.0",
    "terser": "^5.15.0",
    "decode-uri-component": "^0.2.1",
    "json5": "^2.2.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@skatteetaten/frontend-components": "^6.0.3",
    "react": "^17.0.2",
    "react-localization": "^1.0.19",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@storybook/addon-a11y": "^6.5.15",
    "@storybook/addon-docs": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addons": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^7.6.10",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/theming": "^6.5.15",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.18.9",
    "@types/react-scroll": "^1.8.3",
    "@types/styled-components": "^5.1.21",
    "@types/webpack": "^5.28.0",
    "@types/webpack-merge": "^5.0.0",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "airbnb-js-shims": "^2.2.1",
    "babel-loader": "^9.1.2",
    "babel-plugin-styled-components": "^2.0.7",
    "chromatic": "^6.4.1",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.6",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",

{
  "name": "fdk-kit",
  "description": "A collection of reusable UI components for Felles datakatalog.",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:fellesdatakatalog/fdk-kit.git"
  },
  "author": {
    "name": "Alexander Zakharov (azaxarov)",
    "email": "alexander.zakharov@capgemini.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 12.16.3"
  },
  "scripts": {
    "lint:project": "eslint --ext tsx,ts,jsx,js,json,md,html ./",
    "lint:project:fix": "eslint --ext tsx,ts,jsx,js,json,md,html ./ --fix",
    "lint:staged:fix": "eslint --ext tsx,ts,jsx,js,json,md,html --fix",
    "type:check": "tsc",
    "test": "run-s clean:coverage test:jest",
    "test:verbose": "run-s clean:coverage test:jest:verbose",
    "test:coverage": "run-s clean:coverage test:jest:coverage",
    "test:jest": "jest --passWithNoTests",
    "test:jest:verbose": "jest --verbose",
    "test:jest:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:watch:all": "jest --watchAll",
    "test:watch:verbose": "jest --watch --verbose",
    "test:watch:all:verbose": "jest --watchAll --verbose",
    "clean:coverage": "rm -rf coverage",
    "commit": "git-cz",
    "storybook": "start-storybook -p 6006 --ci",
    "storybook:build": "build-storybook"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "react": "^16.13.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.9.1",
    "@storybook/addon-a11y": "^6.0.0-beta.23",
    "@storybook/addon-controls": "^6.0.0-beta.23",
    "@storybook/addon-essentials": "^6.0.0-beta.23",
    "@storybook/addon-links": "^6.0.0-beta.23",
    "@storybook/addons": "^6.0.0-beta.23",
    "@storybook/react": "^6.0.0-beta.23",
    "@storybook/theming": "^6.0.0-beta.23",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.12",
    "@types/styled-components": "^5.1.0",
    "@types/webpack": "^4.41.17",
    "@types/webpack-merge": "^4.1.5",
    "@types/webpack-node-externals": "^1.7.1",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.1.2",
    "copy-webpack-plugin": "^6.0.2",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "iframe-resizer": "^4.2.11",
    "jest": "^26.0.1",
    "jest-styled-components": "^7.0.2",
    "jest-svg-transformer": "^1.0.0",
    "lerna": "^3.22.1",
    "lint-staged": "^10.2.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-dom": "^16.13.1",
    "react-frame-component": "^4.1.2",
    "react-svg-loader": "^3.0.3",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  }
}
